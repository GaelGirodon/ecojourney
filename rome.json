{
  "$schema": "./node_modules/rome/configuration_schema.json",
  "linter": {
    "enabled": true,
    "rules": {
      "recommended": true,
      "complexity": {
        "useSimplifiedLogicExpression": "warn"
      },
      "performance": {
        "noDelete": "warn"
      },
      "suspicious": {
        "noExplicitAny": "off"
      }
    }
  }
}
